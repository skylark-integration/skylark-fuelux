{
  "name": "skylark-fuelux",
  "description": "A version of fuelux that ported to running on skylarkjs",
  "version": "0.9.2",
  "keywords": [
    "css",
    "less",
    "mobile-first",
    "responsive",
    "front-end",
    "framework",
    "web"
  ],
  "homepage": "https://github.com/skylarkui/skylark-fuelux/",
  "author": "Hudaokeji, Inc.",
  "scripts": {
    "build": "sbundle build ."
  },
  "style": "dist/css/bootstrap.css",
  "less": "less/bootstrap.less",
  "main": "./dist/js/main",
  "repository": {
    "type": "git",
    "url": "https://github.com/skylarkui/skylark-fuelux.git"
  },
  "bugs": {
    "url": "https://github.com/skylarkui/skylark-fuelux/issues"
  },
  "license": "MIT",
  "dependencies": {
    "skylark-bootstrap3": "git+https://github.com/skylark-integration/skylark-bootstrap3.git#master",
    "skylark-domx": "git+https://github.com/skylark-domx/skylark-domx.git#master",
    "skylark-domx-repeater": "git+https://github.com/skylark-domx/skylark-domx-repeater.git#master",
    "skylark-domx-scrolls": "git+https://github.com/skylark-domx/skylark-domx-spy.git#master",
    "skylark-domx-spy": "git+https://github.com/skylark-domx/skylark-domx-spy.git#master",
    "skylark-domx-toggles": "git+https://github.com/skylark-domx/skylark-domx-toggles.git#master",
    "skylark-langx": "git+https://github.com/skylark-langx/skylark-langx.git#master"
  },
  "skylarkjs": {
    "build": {
      "packages": {},
      "prepare": {
        "texttojs": [
          "html",
          "json",
          "css"
        ],
        "jsxtojs": false,
        "es6toamd": false,
        "cjstoamd": false
      },
      "bundle": {
        "standard": {
          "include": [
            "skylark-bootstrap3",
            "skylark-domx-spy",
            "skylark-domx-popups",
            "skylark-domx-panels"
          ]
        }
      }
    }
  },
  "engines": {
    "node": ">=0.10.1"
  }
}
